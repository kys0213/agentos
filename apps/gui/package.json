{
  "name": "@agentos/apps-gui",
  "version": "1.0.0",
  "description": "AgentOS GUI Application",
  "author": "AgentOS Team",
  "private": true,
  "main": "dist/main/main.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "build": "tsc && pnpm run build:renderer",
    "build:renderer": "vite build && cp public/index.html dist/renderer/",
    "watch:renderer": "vite build --watch",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:win": "pnpm run build && electron-builder --win",
    "build:linux": "pnpm run build && electron-builder --linux",
    "build:all": "pnpm run build && electron-builder --mac --win --linux",
    "build:docker": "docker build -t agentos-gui .",
    "dev": "concurrently \"tsc -w\" \"NODE_ENV=development electron .\"",
    "lint": "eslint src --ext .ts,.tsx",
    "start": "electron .",
    "test": "jest"
  },
  "build": {
    "appId": "com.agentos.gui",
    "productName": "AgentOS GUI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "public/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@agentos/core": "workspace:*",
    "@chakra-ui/icons": "^2.1.3",
    "@chakra-ui/react": "^2.9.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "electron": "^37.2.4",
    "electron-store": "^8.2.0",
    "framer-motion": "^10.18.0",
    "llm-bridge-spec": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.8",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^7.0.6"
  },
  "volta": {
    "node": "22.17.1"
  }
}
