{
  "name": "@agentos/apps-gui",
  "version": "1.0.0",
  "description": "AgentOS GUI Application",
  "author": "AgentOS Team",
  "private": true,
  "main": "dist/main/main.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "build": "tsc && pnpm run build:renderer",
    "build:dev": "NODE_ENV=development vite build",
    "build:renderer": "vite build",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:win": "pnpm run build && electron-builder --win",
    "build:linux": "pnpm run build && electron-builder --linux",
    "build:all": "pnpm run build && electron-builder --mac --win --linux",
    "dev": "NODE_ENV=development electron .",
    "dev:web": "vite",
    "lint": "eslint src --ext .ts,.tsx",
    "start": "electron .",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "build": {
    "appId": "com.agentos.gui",
    "productName": "AgentOS GUI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "public/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@agentos/core": "workspace:*",
    "@chakra-ui/icons": "^2.1.3",
    "@chakra-ui/react": "^2.9.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tanstack/react-query": "^5.84.0",
    "@tanstack/react-query-devtools": "^5.84.0",
    "@types/react-virtualized-auto-sizer": "^1.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^37.2.4",
    "electron-store": "^8.2.0",
    "framer-motion": "^10.18.0",
    "kbar": "0.1.0-beta.48",
    "llm-bridge-spec": "^1.0.2",
    "lucide-react": "^0.536.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.8",
    "postcss": "^8.5.6",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.3.3",
    "vite": "^7.0.6"
  },
  "volta": {
    "node": "22.17.1"
  }
}
