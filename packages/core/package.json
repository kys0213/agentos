{
  "name": "@agentos/core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "esm/index.js",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -w",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf esm"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.30",
    "jest": "^29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "llm-bridge-spec": "^1.0.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "llm-bridge-spec": "^1"
  },
  "volta": {
    "node": "22.17.1"
  }
}
