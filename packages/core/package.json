{
  "name": "@agentos/core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./agent": {
      "import": "./esm/agent/index.js",
      "require": "./dist/agent/index.js",
      "types": "./dist/agent/index.d.ts"
    },
    "./chat": {
      "import": "./esm/chat/index.js",
      "require": "./dist/chat/index.js",
      "types": "./dist/chat/index.d.ts"
    },
    "./common": {
      "import": "./esm/common/index.js",
      "require": "./dist/common/index.js",
      "types": "./dist/common/index.d.ts"
    },
    "./knowledge": {
      "import": "./esm/knowledge/index.js",
      "require": "./dist/knowledge/index.js",
      "types": "./dist/knowledge/index.d.ts"
    },
    "./llm": {
      "import": "./esm/llm/index.js",
      "require": "./dist/llm/index.js",
      "types": "./dist/llm/index.d.ts"
    },
    "./memory": {
      "import": "./esm/memory/index.js",
      "require": "./dist/memory/index.js",
      "types": "./dist/memory/index.d.ts"
    },
    "./preset": {
      "import": "./esm/preset/index.js",
      "require": "./dist/preset/index.js",
      "types": "./dist/preset/index.d.ts"
    },
    "./tool": {
      "import": "./esm/tool/index.js",
      "require": "./dist/tool/index.js",
      "types": "./dist/tool/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "main": "dist/index.js",
    "module": "esm/index.js",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -w",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:vitest": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf esm"
  },
  "dependencies": {
    "@agentos/lang": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^20.17.30",
    "llm-bridge-loader": "0.0.6",
    "llm-bridge-spec": "^1.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "llm-bridge-loader": "0.0.6",
    "llm-bridge-spec": "^1"
  },
  "volta": {
    "node": "22.17.1"
  }
}
