fun startSchemeIntent(url: String): Boolean {
    val schemeIntent: Intent = try {
        Intent.parseUri(url, Intent.URI_INTENT_SCHEME) // Intent ìŠ¤í‚´ì„ íŒŒì‹±
    } catch (e: URISyntaxException) {
