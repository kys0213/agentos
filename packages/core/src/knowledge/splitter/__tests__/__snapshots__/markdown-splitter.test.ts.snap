    } catch (e: ActivityNotFoundException) { // ì•±ì´ ì„¤ì¹˜ ì•ˆ ë˜ì–´ ìˆëŠ” ê²½ìš°
        val packageName = schemeIntent.getPackage()

        if (!packageName.isNullOrBlank()) {
            startActivity(
                Intent(
                    Intent.ACTION_VIEW,
                    Uri.parse("market://details?id=$packageName") // ìŠ¤í† ì–´ë¡œ ì´ë™
                )
            )
            return true
        }
    }
    return false
}
