{
  "name": "@agentos/lang",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "esm/index.js",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -w",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf esm"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "escape-string-regexp": "^5.0.0",
    "@datastructures-js/deque": "^1.0.5",
    "@datastructures-js/linked-list": "^6.1.3",
    "@datastructures-js/queue": "^4.0.2",
    "slugify": "^1.6.6",
    "string-width": "^7.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "22.17.1"
  }
}
