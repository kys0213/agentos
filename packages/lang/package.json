{
  "name": "@agentos/lang",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./date": {
      "import": "./esm/date/index.js",
      "require": "./dist/date/index.js",
      "types": "./dist/date/index.d.ts"
    },
    "./fs": {
      "import": "./esm/fs/index.js",
      "require": "./dist/fs/index.js",
      "types": "./dist/fs/index.d.ts"
    },
    "./json": {
      "import": "./esm/json/index.js",
      "require": "./dist/json/index.js",
      "types": "./dist/json/index.d.ts"
    },
    "./string": {
      "import": "./esm/string/index.js",
      "require": "./dist/string/index.js",
      "types": "./dist/string/index.d.ts"
    },
    "./utils": {
      "import": "./esm/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./validation": {
      "import": "./esm/validation/index.js",
      "require": "./dist/validation/index.js",
      "types": "./dist/validation/index.d.ts"
    },
    "./datastructures": {
      "import": "./esm/datastructures/index.js",
      "require": "./dist/datastructures/index.js",
      "types": "./dist/datastructures/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "esm/index.js",
    "types": "dist/index.d.ts"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -w",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf esm",
    "verify:esm": "node ./scripts/verify-esm.mjs",
    "verify:cjs": "node ./scripts/verify-cjs.cjs"
  },
  "dependencies": {
    "@datastructures-js/deque": "^1.0.5",
    "@datastructures-js/linked-list": "^6.1.3",
    "@datastructures-js/queue": "^4.0.2",
    "@innei/string-width": "7.1.1-fork.0",
    "change-case-all": "^2.1.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "is": "^3.3.2",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "22.17.1"
  }
}
